  "main": "server.js",
  "scripts": {
    "build": "/bin/sh tsc_eval.sh",
    "start": "npm run reset:db && ts-node src/server.ts",
    "reset:db": "npx sequelize db:drop && npx sequelize db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "dev": "nodemon --watch \"./src/**\" ./src/server.ts",
    "test:integration": "mocha --require ts-node/register tests/integration/*test.ts --exit",
    "test:units": "mocha --require ts-node/register tests/units/*test.ts --exit",
    "test:coverage": "nyc npm run test"
  },
